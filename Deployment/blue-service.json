{
   "kind":"Service",
   "apiVersion":"v1",
   "metadata":{
      "name":"bluegreenlb",
      "labels":{
         "app":"bluegreenlb"
      }
   },
   "spec":{
      "ports": [
         {
            "port":80,
            "targetPort":80,
            "name": "http"
         },
         {
            "port":443,
            "targetPort":80,
            "name": "https"
         }
      ],
      "selector":{
         "app":"blue"
      },
      "type": "LoadBalancer"
   }
}